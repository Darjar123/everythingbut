{% include theme_path ~ "/templates/header.html.twig" %}
<div id="no1_content" class="wrap layout-m">
	<div id="top_main" class="col-md-12">
		{% if page.section %}
			{{ page.section }}
		{% endif %}
	</div>
	<div class="container">
		<div class="row">
			<div id="no1_main" class="col-md-12 col-main">
				<div id="no1_mainmidle">
					{% if page.breadcrumb %}
						<div id="no1_breadcrumbs" class="wrap">
							<div class="container">
								<div class="row">
									<div class="col-md-12">
										<div id="no1_titlepage"></div>
										<div id="no1_pathway" class="clearfix">
											<div class="pathway-inner">
												<span class="icon-pointer "></span>
												{{ page.breadcrumb }}
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					{% endif %}
					{% if page.content %}
						{{ page.content }}
					{% endif %}
				</div>
			</div>
		</div>
		
	</div>
</div>
{% include theme_path ~ "/templates/footer.html.twig" %}